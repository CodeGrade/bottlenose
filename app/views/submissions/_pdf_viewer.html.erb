<div id="pdf_<%= index %>" class="pdfDisplay" dir="ltr" data-source="<%= url %>" data-rendered="<%= source || url %>" data-alt="<%= alt %>">
  <div class="pdfToolbar">
    <div class="toolbarViewerMiddle">
      <div class="splitToolbarButton">
        <button id="zoomOut" class="toolbarButton zoomOut" title="Zoom Out" tabindex="21">
          <i class="glyphicon glyphicon-minus"></i>
        </button>
        <div class="splitToolbarButtonSeparator"></div>
        <button id="zoomIn" class="toolbarButton zoomIn" title="Zoom In" tabindex="22">
          <i class="glyphicon glyphicon-plus"></i>
        </button>
      </div>
      <span id="scaleSelectContainer" class="dropdownToolbarButton">
        <select id="scaleSelect" title="Zoom" tabindex="23">
          <option value="auto" selected="selected">Automatic Zoom</option>
          <option value="page-actual">Actual Size</option>
          <option value="page-fit">Page Fit</option>
          <option value="page-width">Page Width</option>
          <option value="0.5">50%</option>
          <option value="0.75">75%</option>
          <option value="1">100%</option>
          <option value="1.25">125%</option>
          <option value="1.5">150%</option>
          <option value="2">200%</option>
        </select>
      </span>
    </div>
    <div class="toolbarViewerRight">
      <div class="splitToolbarButton">
        <button class="toolbarButton" id="new_window" title="Open in new window">
          <i class="glyphicon glyphicon-new-window"></i>
        </button>
        <button class="toolbarButton" id="download" title="Download file">
          <i class="glyphicon glyphicon-save-file"></i>
        </button>
      </div>
    </div>
    <div class="loadingBar">
      <div class="progress">
        <div class="glimmer">
        </div>
      </div>
    </div>
  </div>
  <div class="pdfPages"></div>
</div>
