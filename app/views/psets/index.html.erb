<p id="notice"><%= notice %></p>

<h1>Psets</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @psets.each do |pset| %>
      <tr>
        <td><%= link_to 'Show', pset %></td>
        <td><%= link_to 'Edit', edit_pset_path(pset) %></td>
        <td><%= link_to 'Destroy', pset, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pset', new_pset_path %>
